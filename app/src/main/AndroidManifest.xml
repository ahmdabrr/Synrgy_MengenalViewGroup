<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.synrgy_mengenalviewgroup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ConstraintLayout"></activity>
        <activity android:name=".activity.TableLayout" />
        <activity android:name=".activity.CardView" />
        <activity android:name=".activity.FrameLayout" />
        <activity android:name=".activity.GridLayout" />
        <activity android:name=".activity.RelativeLayout" />
        <activity android:name=".activity.ScrollView" />
        <activity android:name=".activity.LinearLayoutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>